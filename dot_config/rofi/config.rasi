/* https://davatorium.github.io/rofi/CONFIG/ */

@theme "~/.local/share/rofi/themes/main.rasi"

configuration {
  drun-use-desktop-cache: true;
  drun-display-format: "{name}";
  run-command: "swaymsg exec '{cmd}'";

  kb-element-next: "Alt+numbersign";
  kb-element-prev: "Alt+odiaeresis";
  kb-custom-5: "Alt+3";
  kb-custom-6: "Alt+1";
  kb-custom-7: "Alt+2";
  /* unbind to free keybinding */
  kb-custom-1: "";
  kb-custom-2: "";
  kb-custom-3: "";
  kb-custom-13: "";
}
